<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
  <SelectDialog
      id="valueHelpContratoDialog"
      title="{i18n>Seleccionar_Contrato}"
      multiSelect="true"
      confirm="onValueHelpContratoCancel"
      cancel="onValueHelpContratoCancel"
      search="onSearchContrato"
      items="{VHModel>/VHContratos}">
    <StandardListItem
        title="{VHModel>Contrato}"
        description="{VHModel>Descripcion}"
        icon="sap-icon://value-help" />
  </SelectDialog>
</core:FragmentDefinition>